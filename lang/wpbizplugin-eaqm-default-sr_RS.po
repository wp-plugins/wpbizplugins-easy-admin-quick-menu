msgid "No menu button found"
msgstr "Taster menija nije pronađen"

#: inc/custom-posttypes.php:48
msgid "No menu buttons found in the trash"
msgstr "U korpi za otpatke nema tastera menija"

#: inc/custom-posttypes.php:215 inc/custom-posttypes.php:216
#: inc/custom-posttypes.php:217 inc/custom-posttypes.php:218
msgid "Menu button updated."
msgstr "Taster menija ažuriran"

#: inc/custom-posttypes.php:221 inc/custom-posttypes.php:223
msgid "Menu button added."
msgstr "Taster menija dodat"

#: inc/custom-posttypes.php:222
